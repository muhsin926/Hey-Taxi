{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:react/recommended",
        "standard-with-typescript",
        "plugin:react/jsx-runtime"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "warn",
             "react/prop-types": "off",
             "react/self-closing-comp": "warn",
             "padding-line-between-statements": [
                           "error",
                           {
                               "blankLine": "always",
                               "prev": "*",
                               "next": "return"
                           },
                           {
                               "blankLine": "always",
                               "prev": [
                                   "const",
                                   "let",
                                   "var"
                               ],
                               "next": "*"
                           },
                          {
                               "blankLine": "any",
                               "prev": [
                                   "const",
                                   "let",
                                   "var"
                               ],
                               "next": [
                                   "const",
                                   "let",
                                   "var"
                               ]
                           }
                       ]
                
    },
    "settings": {
             "react": {
               "version": "detect"
             }
            }
}
